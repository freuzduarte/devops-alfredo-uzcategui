pipeline {
    agent any
    stages {
        stage('Starting') {
            steps {
                script {
                    println 'Starting Project'
                }
            }
        }
        stage(name: 'Build') {
            steps {
                script {
                    println 'Building Project'
                }
            }
        }
    }
}